<!DOCTYPE html>
<html>
<head>
  <title>TheQ</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <header>
    <h1><%= link_to 'the-q', root_path %></h1>
    <% if session['user'] %>
      <a href='/auth/logout'>Log out <%= session['user'] %></a>
    <% end %>
  </header>

<%= yield %>

</body>
</html>
